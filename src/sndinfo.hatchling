hatchling/melee1 "Sounds/Hatchling/Melee1.ogg"
hatchling/melee2 "Sounds/Hatchling/Melee2.ogg"
hatchling/melee3 "Sounds/Hatchling/Melee3.ogg"
hatchling/melee4 "Sounds/Hatchling/Melee4.ogg"

$random skull/melee {
  hatchling/melee1
  hatchling/melee2
  hatchling/melee3
  hatchling/melee4
}
$volume skull/melee 0.6

// Is an active idle sound:
$random skull/idlescream {
  play/nothing
  play/nothing
  play/nothing
  skull/melee
}
$volume skull/idlescream 0.6

