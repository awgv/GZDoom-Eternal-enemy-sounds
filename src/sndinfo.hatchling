hatchling/death2 "Sounds/Hatchling/Death2.ogg"
hatchling/death3 "Sounds/Hatchling/Death3.ogg"
hatchling/death4 "Sounds/Hatchling/Death4.ogg"
hatchling/death5 "Sounds/Hatchling/Death5.ogg"

$random skull/death {
  hatchling/death1
  hatchling/death2
  hatchling/death3
  hatchling/death4
  hatchling/death5
}
$volume skull/death 0.6
$alias skull/pain skull/death
